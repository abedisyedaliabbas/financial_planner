{
  "name": "financial-planner-public",
  "version": "1.0.0",
  "description": "Public freemium financial planning and expense tracking service",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "seed": "node server/seed.js"
  },
  "keywords": ["finance", "budget", "expense-tracker", "freemium", "saas"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "pg": "^8.11.3",
    "body-parser": "^1.20.2",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcrypt": "^5.1.1",
    "stripe": "^14.7.0",
    "express-rate-limit": "^7.1.5",
    "validator": "^13.11.0",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}

